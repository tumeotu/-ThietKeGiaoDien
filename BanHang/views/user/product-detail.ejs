<header>

  <link href="user/css/product-detail.css" rel="stylesheet">
  <script src="user/js/product-detail.js"></script>
  <!--  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>-->
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script>
    function addCart(){
      const num = document.getElementById('<%- product._id %>AMOUNT').value;
      const url =' http://127.0.0.1:3000/products-detail?id=<%= product._id %>&&add=' + num;
      return window.location= url;
    }

    function try_close(id)
    {
      let url = 'http://127.0.0.1:3000/try-clothes?product=' + id;
      return window.location= url;
    }

    function quick_buy(id)
    {
      const num = document.getElementById('<%- product._id %>AMOUNT').value;
      // const num = getElementById('<%- product._id %>AMOUNT').value;
      // const url ='http://127.0.0.1:3000/quick-buy?id=' + id  + '&&amount=' + num;
      let url = 'http://127.0.0.1:3000/quick-buy?id=' + id  + '&&amount=' + num;
      return window.location= url;
    }

  </script>
</header>

